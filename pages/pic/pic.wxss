.box-pic .box-pic-list-multi {
    overflow: hidden;
    margin-top: 228rpx;
}

.box-pic .box-pic-list-multi .box-ul {
    width: 100%;
}

.box-pic .box-pic-list-multi .box-li {
    font-size: 0;
    overflow: hidden;
    margin-bottom: 20rpx;
    width: 343rpx;
    border-radius: 10rpx 10rpx 0 0;
    border: 1rpx solid #f5f5f5;
}

.box-pic .box-pic-list-multi .box-ul .fl {
    width: 350rpx;
    float: left;
    overflow: hidden;
}

.box-pic .box-pic-list-multi .box-ul .fl .box-li {
    float: left;
}

.box-pic .box-pic-list-multi .box-ul .fr {
    width: 350rpx;
    float: right;
    overflow: hidden;
}

.box-pic .box-pic-list-multi .box-ul .fr .box-li {
    float: right;
}

.box-pic .box-pic-list-multi .box-li image {
    width: 100%;
}

.box-pic .box-pic-list-multi .box-li .box-title {
    font-size: 24rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    line-height: 40rpx;
    color: #535253;
    padding: 10rpx 20rpx 0;
}

.box-pic .box-pic-list-multi .box-li .box-attr {
    font-size: 24rpx;
    color: #535253;
    padding: 10rpx 20rpx;
    line-height: 40rpx;
    overflow: hidden;
}

.box-pic .box-pic-list-multi .box-li .box-attr .ba-left {
    float: left;
    width: 150rpx;
}

.box-pic .box-pic-list-multi .box-li .box-attr .ba-right {
    float: right;
    width: 150rpx;
    text-align: right;
}

