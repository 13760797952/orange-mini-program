}

.box-pic .box-pic-list-single .box-li {
    font-size: 0;
    overflow: hidden;
    margin-bottom: 20rpx;
    width: 343rpx;
    border-radius: 10rpx;
    border: 1rpx solid #f5f5f5;
    position: relative;
}

.box-pic .box-pic-list-single .box-ul .fl {
    width: 350rpx;
