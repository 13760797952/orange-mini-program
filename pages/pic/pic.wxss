.box-pic-header {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 80;
}

.box-tab-pic {
    background: #fff;
    overflow: hidden;
    display: flex;
    width: 100%;
}

.box-tab-pic .box-li {
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 0;
}

.box-tab-pic .box-li .box-item {
    width: 234rpx;
    font-size: 32rpx;
    color: #333;
    line-height: 106rpx;
    text-align: center;
    display: inline-block;
    border-bottom: 6rpx solid #fff;
}

.box-tab-pic .box-li.active .box-item {
    color: #2fbd80;
    border-color: #2fbd80;
}

.box-pic-multi {
    position: relative;
}

.box-pic-multi .box-options {
    background: #fff;
    display: flex;
    width: 100%;
    border-top: 2rpx solid #f5f5f5;
    border-bottom: 2rpx solid #f5f5f5;
}

.box-pic-multi .box-options .box-li {
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 0;
}

.box-pic-multi .box-options .box-li .box-item {
    font-size: 24rpx;
    color: #333;
    line-height: 90rpx;
    text-align: center;
    display: inline-block;
}

.box-pic-multi .box-options .box-li .box-item .icon {
    font-size: 20rpx;
    margin-left: 4rpx;
}

.box-pic-multi .box-options-body {
    background: #fff;
    position: absolute;
    top: 96rpx;
    left: 0;
    right: 0;
    overflow: hidden;
}

.box-pic-multi .box-options-body .box-ul {
    width: 110%;
}

.box-pic-multi .box-options-body .box-li {
    width: 170rpx;
    height: 60rpx;
    line-height: 60rpx;
    margin: 20rpx 38rpx;
    color: #404040;
    border: 2rpx solid #eee;
    float: left;
    border-radius: 6rpx;
    font-size: 24rpx;
}

.box-pic-multi .box-options-body .box-li.active {
    border-color: #2fbd80;
    color: #2fbd80;
}

