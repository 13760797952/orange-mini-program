.box-pic .box-pic-list-single .box-li image {
    width: 100%;
}

.box-pic .box-pic-list-single .box-li .box-attr {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    font-size: 24rpx;
    color: #fff;
    padding: 10rpx 20rpx;
    line-height: 40rpx;
    overflow: hidden;
}

.box-pic .box-pic-list-single .box-li .box-attr .ba-right {
    float: right;
    width: 150rpx;
    text-align: right;
}

.box-shadow {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,.3);
    z-index: 70;
}