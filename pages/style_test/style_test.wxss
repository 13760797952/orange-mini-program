.problem .list {
  margin: 0;
  padding: 0;
  background: transparent;
  text-align: center;
  font-size: 25rpx;
  color: #333;
  overflow: hidden;
  box-sizing: border-box;
}
.problem .list .choice-item {
  position: relative;
  float: left;
  width: 50%;
  line-height: 42rpx;
  box-sizing: border-box;
}
.problem .list .choice-item .after,
.problem .list .choice-item.active .before {
  display: none;
}
.problem .list .choice-item .radio  {
  display: block;
  margin: 7rpx 0;
}
.problem .list .choice-item .radio:before {
  content: "";
  display: inline-block;
  margin: -4px 7rpx 0 0;
  width: 32rpx;
  height: 32rpx;
  border: 2px solid #333;
  border-radius: 50%;
  background: transparent;
  vertical-align: middle;
}
.problem .list .choice-item radio {
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}
.problem .list .choice-item.active .after {
  display: block;
  margin: 0 auto;
}
.problem .list .choice-item.active .radio:before {
  border-color: #ef5b01;
  background: url(//static.o-home.com/v3/views/wap/page/questionnaire2/img/cek_7ea753e.png) no-repeat center center / 30rpx #ef5b01;
}
