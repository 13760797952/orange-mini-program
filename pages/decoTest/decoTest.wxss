/**index.wxss**/
page{
height: 100%;
}
.show{
display: block;
}
.hide{
display: none;
}

@font-face {
  font-family: "zhanku";
  src: url("//static.o-home.com/v3/fonts/zhanku_bc7643f.eot"); /* IE9 */
  src: url("//static.o-home.com/v3/fonts/zhanku_bc7643f.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
  url("//static.o-home.com/v3/fonts/zhanku_18a28ad.woff") format("woff"), /* chrome、firefox */
  url("//static.o-home.com/v3/fonts/zhanku_dcf0d64.ttf") format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
  url("//static.o-home.com/v3/fonts/zhanku_2c2a93d.svg#zhanku") format("svg"); /* iOS 4.1- */
  font-style: normal;
  font-weight: normal;
}
.container{
  height: 100%;
  position: relative;
}
.page{
  height: 100%;
  background: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/bg_170021e.jpg);
  background-size: cover;
  overflow: hidden;
}
.parts{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.parts image{
  width: 488rpx;
  height: 579rpx;
  position: absolute;
  -webkit-animation: float ease-in-out 1.7s infinite;
  animation: float ease-in-out 1.7s infinite;
}
.parts .shafa{
  left: -68rpx;
  top: -132rpx;
  animation-delay: 0.3s;
  animation-duration: 1.7s;
}
.parts .deng{
  right: -150rpx;
  top: -170rpx;
  animation-delay: 0.5s;
  animation-duration: 1.5s;
}
.parts .shuazi{
  left: -210rpx;
  bottom: 210rpx;
  animation-delay: 0.7s;
  animation-duration: 1.6s;
}
.parts .qizi{
  left: -165rpx;
  bottom: -92rpx;
  animation-delay: 0.9s;
  animation-duration: 1.8s;
}
.parts .baozhen{
  right: -216rpx;
  bottom: -65rpx;
}
.center{
  width: 100%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.home-page .logo{
  display: block;
  width: 167rpx;
  height: 81rpx;
  margin: 0 auto;
}
.home-page .home-text{
  display: block;
  width: 522rpx;
  height: 305rpx;
  margin: 70rpx auto;
}
.home-page .btn-grounp{
  width: 466rpx;
  margin: 0 auto;
  z-index: 99;
}
.button{
  display: block;
  width: 466rpx;
  height: 118rpx;
  background: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/button_c9aab98.png) no-repeat;
  background-size: 100% 100%;
  margin-bottom: 50rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 50rpx;
  font-family: "zhanku";
  margin: 0 auto;
  -webkit-tap-highlight-color:transparent;
}
button::after {
  border: none;
}
.home-page .button{
  margin-bottom: 50rpx;
  color: #5d5d5d;
}
.main-page{
  -webkit-animation: rotate3d ease-in-out 1s;
  animation: rotate3d ease-in-out 1s;
  transform-origin: 50% 50% 0;
}
@keyframes rotate3d {
  0%{
    transform: rotateY(0deg);
  }
  100%{
    transform: rotateY(360deg);
  }
}
.question-item{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  position: relative;
  left: 0;
  top: 0;
  -webkit-animation: zoomIn ease-in-out 0.8s;
  animation: zoomIn ease-in-out 0.8s;
}
@keyframes zoomIn {
  0%{
    transform: scale(0);
  }
  100%{
    transform: scale(1)
  }
}
.qs-box{
  padding: 23rpx;
  background: #fff;
  box-shadow: 0 0 21rpx rgba(250,136,55,0.75);
  border-radius: 10rpx;
}
.qs-pic{
  height: 440rpx;
  position: relative;
}
.speak{
  width: 272rpx;
  height: 231rpx;
  position: absolute;
  top: 18rpx;
  left: 9rpx;
}
.speak-experience{
  width: 261rpx;
  height: 233rpx;
  top: 0;
  left: 38rpx;
}
.speak-time,.speak-together{
  top: 9rpx;
  left: auto;
  right: 9rpx;
}
.speak-style{
  width: 247rpx;
  top: 90rpx;
  left: auto;
  right: 20rpx;
}
.qs-text{
  font-family: "zhanku";
  font-size: 52rpx;
  color: #5d5d5d;
  line-height: 60rpx;
  padding: 15rpx 0;
}
.qs-work{
  font-size: 40rpx;
}
.choices{
  margin-top: 36rpx;
}
.ch-btn{
  display: block;
  width: 466rpx;
  height: 118rpx;
  margin: 0 auto;
  background-image: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/button_c9aab98.png);
  background-size: cover;
  margin-bottom: 22rpx;
  font-size: 42rpx;
  color: #5d5d5d;
  font-family: "zhanku";
  position: relative;
  line-height: 106rpx;
  box-sizing: border-box;
  padding: 4rpx 5rpx;
}
.ch-btn text{
  display: block;
  width: 100%;
  height: 98rpx;
  padding: 0 28rpx;
  border-radius: 40rpx;
  line-height: 98rpx;
  box-sizing: border-box;
}
.ch-btn.active text{
  background-color: #fb8533;
  color: #fff;
}
.choice-style{
  width: 200rpx;
  height: 229rpx;
  position: relative;
  margin: 14rpx;
  background: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/button-square_8bd925a.png) no-repeat;
  background-size: 100% 100%;
}
.choice-style .question{
  padding: 15rpx 0;
}
.choice-style text{
  width: 101%;
  height: 203rpx;
  border-radius: 15rpx;
  top: 4rpx;
  left:3rpx;
  padding: 0;
}
.choice-style image{
  display: block;
  width: 85%;
  height: 90%;
  margin: 0 auto;
  position: absolute;
  left: 7.5%;
  top: 0;
}
.choice-together{
  width: 600rpx;
  border-radius: 48rpx;
  background: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/button-large_e283541.png) no-repeat;
  background-size: 100% 100%;
}
.choice-together  span{
  width: 594rpx;
  padding: 0 0 0 15rpx;
}
ch-btn.active span{
  background: #fb8533;
  color: #fff;
}
.radio-inline{
  display: flex;
  justify-content: space-between;
}
.radio-inline .choice-style{
  flex: 1;
}
.radio-list{
  margin-top: 10rpx;
  color: #555;
}
.radio-list .h3{
  font-size: 30rpx;
}
.question-item radio{
  display: none;
}

/* loading-page */
.loading-page{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: url(//static.o-home.com/v3/views/wap/page/deco_intro/img/bg_170021e.jpg) no-repeat;
  background-size: cover;
}
.loading-page .logo {
  width: 167rpx;
  height: 81rpx;
  position: absolute;
  left: 50%;
  margin-left: -83.5rpx;
  bottom: 60rpx;
}
.loading-page .trans-circle{
  width: 520rpx;
  height: 520rpx;
  background: #fff;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  box-shadow: 0 0 21rpx rgba(250,136,55,0.75);
  margin: 15% auto;
  text-align: center;
  padding: 45rpx 0;
  box-sizing: border-box;
}
.trans-pic{
  width: 252rpx;
  height: 300rpx;
}
.trans-pic image{
  display: block;
  width: 50%;
  margin: 0 auto;
}
.trans-circle >text{
  width: 50%;
  margin: 0 auto;
  font-size: 36rpx;
}
.trans-circle >text .load-num{
  font-style: normal;
  color: #fb620f;
  margin: 0 10rpx;
  line-height: 52rpx;
}
.trans-circle .qtext {
  display: inline-block;
  vertical-align: middle;
  width: 1.5em;
  text-align: left;
  margin-left: .1em;
  font-size: 1.3em;
  line-height: .5
}
.trans-circle .qtext text {
  -webkit-animation: ani-load1 .8s linear infinite both;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden
}
@keyframes ani-load1 {
    0% {
        width: 0
    }

    100% {
        width: 100%
    }
}
.result-page{
  height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling : touch;
}
.result-page .res-wrapper{
  position: relative;
}
.result-page .res-btn{
  width: 246rpx;
  height: 108rpx;
  position: absolute;
  bottom: 192rpx;
  z-index: 10;
}
.result-page .btn1{
  background: url(//media.o-home.com/ohome-php/assets/image/deco_test/btn-1.png) no-repeat;
  background-size: cover;
  left: 120rpx;
}
.result-page .btn2{
  background: url(//media.o-home.com/ohome-php/assets/image/deco_test/btn-2.png) no-repeat;
  background-size: cover;
  right: 120rpx;
}
.result-page canvas{
  position: absolute;
  left: 200%;
  top: 0;
}

@keyframes float {
  0% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
  50% {
    -webkit-transform: translateY(20rpx);
    transform: translateY(20rpx);
  }
  100% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}

/* 自定义模态框 */
/*mask*/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}
/*content*/
.drawer_box {
  width: 74%;
  position: fixed;
  top: 30%;
  left: 13%;
  z-index: 1001;
  margin-top: -30%;
  border-radius: 15rpx;
  padding: 28rpx 40rpx 42rpx;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ff4900), to(#ff6c00));
  box-sizing: border-box;
}
.drawer_title {
  font: 40rpx "microsoft yahei";
  text-align: center;
  margin-bottom: 30rpx;
  color: #fff;
  font-weight: bold;
}
.drawer_content {
  margin-bottom: 30rpx;
}
.drawer_content .title {
  font-size: 30rpx;
  text-align: left;
  color: #333;
  margin-top: -68rpx;
  display: block;
  padding: 0 20rpx;
}
.drawer_content .modal-pic{
  display: block;
  width: 187rpx;
  height: 173rpx;
  margin: 0 auto 50rpx;
}
.drawer_content .modal-pic2{
  display: block;
  width: 232rpx;
  height: 246rpx;
  margin: 0 auto 50rpx;
}
.drawer_content .tips-text{
  color: #fff;
  font-size: 28rpx;
  font-weight: normal;
  word-break: break-all;
}
.form_group {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 0 auto 1.3em;
  font-size: 32rpx;
  font-weight: bold;
}
.form_group label{
  display: inline-block;
  line-height: 65rpx;
  vertical-align: middle;
}
.form_group input{
  width: 100%;
  height: 90rpx;
  background: #fff;
  border-radius: 15rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: normal;
}
.placeholderStyle{
  color: #5d5b5b;
}
.form_group .sendmsg_input {
  width: 258rpx;
  color: #333;
}
.btn_ok {
  font-size: 36rpx;
  line-height: 90rpx;
  padding: 0 34rpx;
  border-radius: 15rpx;
  background: #ffff00;
  color: #ff4800;
}
.btn_close{
  width: 87rpx;
  height: 87rpx;
  border-radius: 50%;
  border: 1px solid #fff;
  position: absolute;
  left: 50%;
  margin-left: -43.5rpx;
  bottom: -130rpx;
  transform: rotate(45deg)
}
.btn_close:before{
  content: "";
  display: block;
  width: 61rpx;
  height: 6rpx;
  background: #fff;
  border-radius: 3rpx;
  position: absolute;
  left: 13rpx;
  top: 40rpx;
}
.btn_close:after{
  content: "";
  display: block;
  width: 6rpx;
  height: 61rpx;
  background: #fff;
  border-radius: 3rpx;
  position: absolute;
  left: 40rpx;
  top: 13rpx;
}
.share_box{
  width: 80%;
  height: 1000rpx;
  position: absolute;
  top: 0;
  left: 10%;
  z-index: 1001;
}
.share_box image{
  width: 100%;
  height: 1200rpx;
}